extend ../base.pug

block variables
  - var template = 'projets'

block vars 
  - var title =projets.data.meta_title
  - var description =projets.data.meta_description
  - var link__1 = '/galerie'
  - var page__1 = 'galerie'
  - var link__2 = '/propos'
  - var page__2 = 'Propos'
  - var link__3 = '/contact'
  - var page__3 = 'Contact'

block content   
  .projets.fading
    include ../partials/menu
    include ../partials/main__navigation
    .projets__holder 
      .projets__holder__description 
      .projets__holder__slider
        canvas.projets__canvas
    include ../partials/footer 