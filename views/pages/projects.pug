extend ../base.pug

block variables
  - var template = 'projets'

block vars 
  - var title =projets.data.meta_title
  - var description =projets.data.meta_description
  - var link__1 = '/galerie'
  - var page__1 = 'galerie'
  - var link__2 = '/propos'
  - var page__2 = 'Propos'
  - var link__3 = '/contact'
  - var page__3 = 'Contact'

block content   
  .projets.fading
    include ../partials/menu
    include ../partials/main__navigation
    .projets__holder 
      h1 liste des projets
      .projets__holder__description
        p Loading.....
      .projets__holder__slider
        nav 
          a.prev(href='#')
            img(src=('images/arrow-left-circle-lined.svg'))
          a.next(href='#')
            img(src=('images/arrow-right-circle-lined.svg'))
    include ../partials/footer 